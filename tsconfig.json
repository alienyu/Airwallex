{
    "compilerOptions": {
      "outDir": "./dist/",
      "baseUrl": ".",
      "paths": {
        "@router/*": ["biz/web/page/BroccoliAndCo/router/*"],
        "@mobx/*": ["biz/web/page/BroccoliAndCo/mobx/*"],
        "buildConf/*": ["build/config/*"],
        "mulThemeConf": ["biz/web/page/BroccoliAndCo/mulThemeConf/index.ts"],
        "ajax": ["biz/web/page/BroccoliAndCo/static/module/ajax.ts"],
        "getMulThemeProps": ["biz/web/page/BroccoliAndCo/static/module/getMulThemeProps.ts"],
        "Constants": ["biz/web/page/BroccoliAndCo/static/constants/index.ts"]
      },
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "skipLibCheck": true,
      "experimentalDecorators": true,
      "allowJs": true 
    },
    "include": [
      "./biz/**/*"
    ],
    "exclued": [
      "node_modules"
    ]
  }